<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
    <li class="media">
      <div class="media-list">
        <img class="media-object img-rounded img-responsive" src="<%= current_user.image.thumb80.url %>"alt="">
      </div>
      <div class="media-body">
        <div>
          <%= user.name %>
        </div>
        <div>
          <p><%= link_to 'プロフィールを見る', user_path(user) %></p>
        </div>
      </div>
    </li>
    <% end %>
  </ul>
    <%= paginate users %>
<% end %>